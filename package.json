    "framer-motion": "^10.12.16",
    "iconsax-react": "^0.0.8",
    "immer": "^10.0.2",
    "jotai": "^2.1.0",
    "jotai-immer": "^0.2.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
